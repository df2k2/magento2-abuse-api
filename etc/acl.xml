<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Cs_AbuseApi::Check" title="Check" sortOrder="10">
                    <resource id="Cs_AbuseApi::Check_save" title="Save Check" sortOrder="10"/>
                    <resource id="Cs_AbuseApi::Check_delete" title="Delete Check" sortOrder="20"/>
                    <resource id="Cs_AbuseApi::Check_update" title="Update Check" sortOrder="30"/>
                    <resource id="Cs_AbuseApi::Check_view" title="View Check" sortOrder="40"/>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Cs_AbuseApi::config_cs_abuseapi" title="Abuse IP DB API"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
